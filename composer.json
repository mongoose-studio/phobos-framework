{
    "name": "mongoose-studio/phobos-framework",
    "description": "Phobos PHP Framework",
    "version": "3.0.3",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "authors": [
        {
            "name": "Marcel Rojas",
            "email": "marcelrojas16@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PhobosFramework\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "PhobosFramework\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage",
        "test:filter": "phpunit --filter"
    }
}
